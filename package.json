{
  "name": "typescript_express",
  "version": "1.0.0",
  "description": "Express server written in Nodejs and TypeScript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all clean lint tsc webpack copy-assets",
    "clean": "ts-node tools/cleanFiles",
    "copy-assets": "ts-node tools/copyAssets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,hbs --exec npm run dev:start",
    "docs": "npm-run-all lint-docs todo typedoc",
    "lint-docs": "eslint -c .eslintrc.js --ext .ts docs --fix",
    "lint-tests": "eslint -c .eslintrc.js --ext .ts tests --fix",
    "lint": "eslint -c .eslintrc.js --ext .ts src tools webpack.config.js --fix",
    "mocha": "mocha -r ts-node/register tests/**/*.test.ts tests/**/*.test.shell.ts",
    "mochaNoShell": "mocha -r ts-node/register tests/**/*.test.ts",
    "mochaShell": "mocha -r ts-node/register tests/**/*.test.shell.ts",
    "start": "node .",
    "test": "npm-run-all lint-tests mocha",
    "todo": "ts-node ./docs/createTodoPage",
    "tsc": "tsc",
    "typedoc": "ts-node ./docs/createDocs",
    "webpack": "webpack --config webpack.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "archiver": "^3.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "handlebars": "^4.7.3",
    "http-errors": "^1.7.3",
    "js-yaml": "^3.13.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.0",
    "@types/chai": "^4.2.9",
    "@types/express": "^4.17.2",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-session": "^1.17.0",
    "@types/http-errors": "^1.6.3",
    "@types/js-yaml": "^3.12.2",
    "@types/mocha": "^7.0.1",
    "@types/shelljs": "^0.8.6",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "glob": "^7.1.6",
    "leasot": "^10.1.0",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
