{
  "name": "typescript_express",
  "version": "1.0.0",
  "description": "Express server written in Nodejs and TypeScript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all clean lint tsc webpack copy-assets",
    "clean": "ts-node tools/cleanFiles",
    "copy-assets": "ts-node tools/copyAssets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,hbs --exec npm run dev:start",
    "lint": "eslint -c .eslintrc.js --ext .ts src --fix",
    "start": "node .",
    "tsc": "tsc",
    "docs": "npm-run-all todo typedoc",
    "todo": "ts-node ./docs/createTodoPage",
    "typedoc": "ts-node ./docs/createDocs",
    "webpack": "webpack --config webpack.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "handlebars": "^4.7.3",
    "http-errors": "^1.7.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-session": "^1.15.16",
    "@types/http-errors": "^1.6.3",
    "@types/shelljs": "^0.8.6",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/eslint-plugin-tslint": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "glob": "^7.1.6",
    "leasot": "^10.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
